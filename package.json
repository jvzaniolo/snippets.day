{
  "name": "snippets.day",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run build:css && remix build",
    "build:css": "tailwindcss -m -i app/styles/app.css -o app/styles/output.css",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i app/styles/app.css -o app/styles/output.css",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@remix-run/react": "^1.2.3",
    "@remix-run/vercel": "^1.2.3",
    "@supabase/supabase-js": "^1.31.1",
    "highlight.js": "^11.4.0",
    "marked": "^4.0.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "remix": "^1.2.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.2.3",
    "@remix-run/serve": "^1.2.3",
    "@types/marked": "^4.0.2",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.5.0",
    "postcss": "^8.4.8",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14.x"
  }
}
